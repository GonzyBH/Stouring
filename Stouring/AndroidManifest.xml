        <activity
			android:name=".TouringListActivity"
			android:label="@string/touringlistactivity_name" >
     	</activity>
     
        <activity
			android:name=".NewConnexionActivity"
			android:label="@string/newconnexionactivity_name" >
     	</activity>
     	
        <activity
			android:name=".TouringPlaceDisplayerActivity"
			android:label="@string/tpdisplayer_name" >
     	</activity>
     	
        
        <!-- Goolge Maps API Key -->
		<meta-data
     		android:name="com.google.android.maps.v2.API_KEY"
